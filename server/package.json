{
  "name": "auto-server",
  "version": "1.0.0",
  "description": "Backend of auto service",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "run:watch": "nodemon --inspect=0.0.0.0:9080 ./dist/server.js --watch ./dist",
    "dev": "npm-run-all --parallel build:watch run:watch",
    "start": "tsc && node ./dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Pogudin Sergey",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "loglevel": "^1.9.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
